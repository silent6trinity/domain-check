#!/bin/bash

# File containing domain names
file="domains.txt"

# Sort the file contents and remove duplicates
sort -u $file -o $file

# Loop through each domain in the file
while read -r domain
do
    # Send a GET request to the domain
    response=$(curl -s -o /dev/null -w "%{http_code}" $domain)

    # Check if the response code is not 404
    if [ "$response" != "404" ]; then
        # Output the domain name and the HTTP status code
        echo "Domain: $domain, HTTP status code: $response"
    fi
done < "$file"
